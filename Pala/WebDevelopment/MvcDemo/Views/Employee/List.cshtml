@using MvcDemo.Models;
@model List<EmployeeViewModel>
<h1>List of all Employees</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th asp_for="Model[0].FirstName" scope="col"></th>
      <th asp_for="Model[0].LastName" scope="col"></th>
      <th asp_for="Model[0].DOB" scope="col"></th>
      <th asp_for="Model[0].Department" scope="col"></th>
      
    </tr>
  </thead>
  <tbody>
 @for(int i =0; i<Model.Count; i++ )
    {
      var emp= Model[i];
      <partial name="_Row" model=@emp/>
   
    }
  </tbody>
</table>